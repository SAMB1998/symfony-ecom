{% include 'base.html.twig' %}

<title>{% block title %}Ajouter le stock{% endblock %}</title>

{% block body %}
<div class="container">
        <h1>Ajouter le stock : {{product.name}}</h1><br>
        {% include 'layout/_flash_message.html.twig'%}
        {{ form_start(form) }}
                {{ form_row(form.qte,{'attr':{"class":"form form-control"}}) }}<br>
